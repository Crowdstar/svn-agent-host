{
  "name": "crowdstar/svn-agent-host",
  "description": "A native messaging host to handle SVN commands received from specific Chrome extension.",
  "homepage": "https://www.glu.com",
  "license": "proprietary",
  "require": {
    "php": ">=7.0",
    "arvenil/ninja-mutex": "~0.6",
    "bugsnag/bugsnag": "~3.13.0",
    "monolog/monolog": "~1.11",
    "mrrio/shellwrap": "~0.4.0",
    "vlucas/phpdotenv": "~2.2"
  },
  "require-dev": {
    "overtrue/phplint": "~1.1.0",
    "phpunit/phpunit": "~6.0",
    "squizlabs/php_codesniffer": ">=2.0"
  },
  "autoload": {
    "psr-4": {
      "CrowdStar\\SVNAgent\\": "src/CrowdStar/SVNAgent/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CrowdStar\\Tests\\SVNAgent\\": "tests/CrowdStar/SVNAgent/"
    }
  },
  "bin": [
    "bin/kill-host-processes.sh",
    "bin/svn-agent-host.php",
    "bin/svn-cleanup.sh",
    "bin/svn-save-credentials.sh"
  ],
  "config": {
    "discard-changes": true,
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "prefer-stable": true
}
